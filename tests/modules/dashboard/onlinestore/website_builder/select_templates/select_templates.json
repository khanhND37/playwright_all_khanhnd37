          "title": "SPT-7_2",
          "description": "Included 3 templates with and w/o tags",
          "types": ["creator"]
        }
      ],
      "create_templates": {
        "spt_7_1": [
          {
            "title": "page template 1"
          },
          {
            "title": "page template 2"
          }
        ],
        "spt_7_2": [
          {
            "title": "page template 4",
            "tags": []
          },
          {
            "title": "Enjoin Coffee In Morning with",
            "tags": ["Tag 1", "Tag 2", "Tag 3", "Tag 4", "Tag 5", "Tag 6"]
          },
          {
            "title": "page template 3",
            "tags": ["Tag 1", "Tag 2", "Tag 3", "Tag 4", "Tag 5", "Tag 6"]
          }
        ]
      },
      "expected_path": "builder/page",
      "addition_libs": ["page template 3", "Enjoin Coffee In Morning with", "page template 4", "page template 1"],
      "expected_tags": [
        {
          "template": "Enjoin Coffee In Morning with",
          "tags": ["Tag 1", "Tag 2", "Tag 3", "Tag 4", "Tag 5", "Tag 6"]
        },
        {
          "template": "page template 3",
          "tags": ["Tag 1", "Tag 2", "Tag 3", "Tag 4", "Tag 5", "Tag 6"]
        }
      ],
      "unpublished_template": "page template 2"
    },
