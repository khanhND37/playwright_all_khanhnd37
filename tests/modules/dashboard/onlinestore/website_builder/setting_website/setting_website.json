        "favicon": "./data/shopbase/anh2.jpg",
        "description": "Testing update change block Meta description preference",
        "title": "Testing update change block title preference",
        "additional_scripts_head": "<meta name='google-site-verification' content='0zXVPzvIJ0cDrjmBaswrechm--lPJDt_KzYKpgn2uxI'/>",
        "additional_scripts_body": "<script type='text/javascript'>\n    let addressNew = 'Me Linh Ha Noi'\n    let phoneNew = '0999 999 999'\n\n\n    let interval;\n    let intervalFooter;\n    let addressLength = 0;\n    let count = 0\n    function checkUrlChange() {\n\n        let theme = JSON.parse(document.getElementById('__INITIAL_STATE__').textContent).theme.handle\n        if (theme == 'inside') {\n\n            var intervalFooter = setInterval(function () {\n                var check = changePageFooterInside();\n                if (check && check > 10) {\n                    clearInterval(intervalFooter);\n                }\n            }, 100);\n\n        }\n        if (theme == 'roller') {\n\n            var intervalFooter = setInterval(function () {\n                var check = changePageFooterRoller();\n                if (check) {\n                    clearInterval(intervalFooter);\n                }\n            }, 100);\n\n            // intervalFooter = setInterval(changePageFooterRoller, 100);\n        }\n\n        if (window.location.pathname == '/pages/contact-us') {\n            interval = setInterval(changePageContactUs, 100);\n        }\n    }\n\n    window.sbsdk.ready(function () {\n        window.sbsdk.page.onLoad(function () {\n            // code here\n            checkUrlChange()\n        });\n    });\n\n    function changePageContactUs() {\n\n        let className = ''\n        let theme = JSON.parse(document.getElementById('__INITIAL_STATE__').textContent).theme.handle\n        if (theme == 'inside') {\n            className = 'other-page-content'\n        }\n        if (theme == 'roller') {\n            className = 'page-content'\n        }\n        if (!className) {\n            return\n        }\n\n        let contactForm = document.getElementsByClassName(className)[0]\n        if (!contactForm) {\n            return\n        }\n        if (!contactForm.getElementsByTagName('ul')[0]) {\n            return\n        }\n        // change phone\n        let phoneOld = contactForm.getElementsByTagName('ul')[0].getElementsByTagName('li')[1].innerHTML\n        if (!phoneOld) {\n            return\n        }\n        let splitPhone = phoneOld.split('</strong>')\n        if (splitPhone.length < 2) {\n            return;\n        }\n        splitPhone[1] = ' ' + phoneNew\n        document.getElementsByClassName(className)[0].getElementsByTagName('ul')[0].getElementsByTagName('li')[1].innerHTML = splitPhone.join('</strong>')\n\n        // change address\n        let addressOld = contactForm.getElementsByTagName('ul')[0].getElementsByTagName('li')[2].innerHTML\n        if (!addressOld) {\n            return\n        }\n        let splitAddress = addressOld.split('</strong>')\n        if (splitAddress.length < 2) {\n            return;\n        }\n        splitAddress[1] = ' ' + addressNew\n        document.getElementsByClassName(className)[0].getElementsByTagName('ul')[0].getElementsByTagName('li')[2].innerHTML = splitAddress.join('</strong>')\n        clearInterval(interval)\n    }\n\n    function changePageFooterInside() {\n        let footer = document.getElementsByClassName('site-footer__information')[0]\n        // change phone\n        let groupTagA = footer.getElementsByTagName('a')\n        if (!groupTagA) {\n            return\n        }\n        let index = 0\n        let checkFindTel = false\n        for (let i = 0; i < groupTagA.length; i++) {\n            if (groupTagA[i].href.includes('tel')) {\n                index = i\n                checkFindTel = true\n            }\n        }\n        if (!checkFindTel) {\n            return\n        }\n\n        let phoneElement = groupTagA[index]\n        if (!phoneElement) {\n            return\n        }\n\n        let hrefPhone = groupTagA[index].href\n        if (hrefPhone) {\n            let splitHref = hrefPhone.split(':')\n            if (splitHref.length >= 2) {\n                splitHref[1] = phoneNew\n                groupTagA[index].href = splitHref.join(':')\n            }\n        }\n\n        // change address\n        if (!footer.getElementsByTagName('p')[0]) {\n            return\n        }\n\n        if (footer.getElementsByTagName('p').length < 1) {\n            return\n        }\n        addressLength = footer.getElementsByTagName('p').length - 1\n\n        // if (footer && footer.getElementsByTagName('p')[addressLength].innerHTML === addressNew && phoneElement && phoneElement.innerHTML === phoneNew) {\n        //     console.log('aa')\n        //     return true\n        // }\n\n        document.getElementsByClassName('site-footer__information')[0].getElementsByTagName('a')[index].innerText = phoneNew\n        document.getElementsByClassName('site-footer__information')[0].getElementsByTagName('p')[addressLength].innerText = addressNew\n        return count++\n    }\n\n    function changePageFooterRoller() {\n        let footer = document.getElementsByClassName('footer-style-none')[0]\n        if (!footer) {\n            return\n        }\n        if (!footer.getElementsByTagName('ul')[0]) {\n            return\n        }\n        // change phone\n        let footPhone = footer.getElementsByTagName('ul')[0].getElementsByTagName('li')[1].innerHTML\n        if (!footPhone || (footPhone && footPhone == phoneNew)) {\n            return true\n        }\n        document.getElementsByClassName('footer-style-none')[0].getElementsByTagName('ul')[0].getElementsByTagName('li')[1].innerHTML = phoneNew\n\n        // change address\n        let footAddress = footer.getElementsByTagName('ul')[0].getElementsByTagName('li')[2].innerHTML\n        if (!footAddress || (footAddress && footAddress == addressNew)) {\n            return true\n        }\n        document.getElementsByClassName('footer-style-none')[0].getElementsByTagName('ul')[0].getElementsByTagName('li')[2].innerHTML = addressNew\n        return true\n    }\n\n</script>\n"
      },
