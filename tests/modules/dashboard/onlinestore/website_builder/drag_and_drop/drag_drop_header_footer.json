    "SB_WEB_BUILDER_DnD_11": {
      "description": "Section_Check di chuyển các section trên Layer giữa Header/Body/Footer",
      "data": {
        "template_name": "WB 6 section",
        "dnd": [
          {
            "description": "Drag section body vào header",
            "from": {
              "sectionName": "Section 3"
            },
            "to": {
              "sectionName": "Section 1"
            },
            "expect": {
              "sections_area": ["Section 3", "Section 1", "Section 2"],
              "color": "rgb(4, 180, 133)",
              "toast": "Section moved to header. It will now be visible on all pages in this store"
            }
          },
          {
            "description": "Drag section header với section header",
            "from": {
              "sectionName": "Section 1"
            },
            "to": {
              "sectionName": "Section 3"
            },
            "expect": {
              "sections_area": ["Section 3", "Section 1", "Section 2"],
              "color": "rgb(4, 180, 133)"
            }
          },
          {
            "description": "Drag section header vào body",
            "from": {
              "sectionName": "Section 1"
            },
            "to": {
              "sectionName": "Section 4"
            },
            "expect": {
              "sections_area": ["Section 1", "Section 4"],
              "color": "rgb(94, 106, 110)"
            }
          },
          {
            "description": "Drag section body vào footer",
            "from": {
              "sectionName": "Section 4"
            },
            "to": {
              "sectionName": "Section 5"
            },
            "expect": {
              "sections_area": ["Section 4", "Section 5", "Section 6"],
              "color": "rgb(4, 180, 133)",
              "toast": "Section moved to footer. It will now be visible on all pages in this store"
            }
          },
          {
            "description": "Drag section footer với section footer",
            "from": {
              "sectionName": "Section 6"
            },
            "to": {
              "sectionName": "Section 4"
            },
            "expect": {
              "sections_area": ["Section 4", "Section 5", "Section 6"],
              "color": "rgb(4, 180, 133)"
            }
          },
          {
            "description": "Drag section header vào section footer",
            "from": {
              "sectionName": "Section 2"
            },
            "to": {
              "sectionName": "Section 4"
            },
            "expect": {
              "sections_area": ["Section 2", "Section 4", "Section 5", "Section 6"],
              "color": "rgb(4, 180, 133)"
            }
          },
          {
            "description": "Drag section footer vào section header",
            "from": {
              "sectionName": "Section 2"
            },
            "to": {
              "sectionName": "Section 3"
            },
            "expect": {
              "sections_area": ["Section 2", "Section 3"],
              "color": "rgb(4, 180, 133)"
            }
          }
        ],
        "webfront": [
          {
            "sections": ["Section 2", "Section 3", "Section 4", "Section 5", "Section 6"],
            "color": "rgb(4, 180, 133)"
          },
          {
            "body": true,
            "sections": ["Section 1"],
            "color": "rgb(0, 150, 237)"
          }
        ]
      }
    },
